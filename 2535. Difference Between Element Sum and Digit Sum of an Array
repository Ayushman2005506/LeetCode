class Solution {
public:
    int differenceOfSum(vector<int>& nums) {
        int x=0;
        int y=0;
        int z;
        for(int i=0;i<nums.size();i++){
            x+=nums[i];
        }
        for(int i=0;i<nums.size();i++){
            while(nums[i]!=0){
            y+=(nums[i]%10);
            nums[i]/=10;
            }

        }
        z=abs(x-y);
        return z;

    }
};
