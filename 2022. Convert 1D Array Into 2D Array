class Solution {
public:
    vector<vector<int>> construct2DArray(vector<int>& original, int m, int n) {
        vector<vector<int>> ans;
        int s = original.size();
        if(m*n != s){
            return ans;
        }
        for(int i=0;i<m;i++){
            vector<int> row;
            for(int j=0;j<n;j++){
                row.push_back(original[n*i+j]);
            }
            ans.push_back(row);
        }
        
        return ans;
    }
};
