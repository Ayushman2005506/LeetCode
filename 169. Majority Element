class Solution {
public:
    int majorityElement(vector<int>& nums) {
        unordered_map<int,int> freq;
        for(int i=0;i<nums.size();i++){
            freq[nums[i]]++;
        }
        int n=nums.size();
        int a=0;
        for(auto& x:freq){
            if(x.second>(n/2)){
                a=x.first;
            }
        }
        return a;
    }
};
