class Solution {
public:
    int minPairSum(vector<int>& nums) {
        int n=nums.size();
        sort(nums.begin(),nums.end());
        int maximum=0;
        for(int i=0;i<n/2;i++){
            int current=nums[i]+nums[n-1-i];
            maximum=max(maximum,current);
        }
        return maximum;
    }
};
