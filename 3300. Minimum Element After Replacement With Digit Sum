class Solution {
public:
    int minElement(vector<int>& nums) {
         vector<int>result;
        for(int i=0;i<nums.size();i++){
            int last_digit=0;
            int sum=0;
            while(nums[i]!=0){
                last_digit=nums[i]%10;
                sum+=last_digit;
                nums[i]/=10;
            }
            result.push_back(sum);
        }
        int min=result[0];
        for(int i=0;i<result.size();i++){
            if(result[i]<min){
                min=result[i];
            }
        }
        return min;
    }
};
