class Solution {
public:
    vector<int> sortedSquares(vector<int>& nums) {
        int n=nums.size();
     
        vector<int> ans;
        for(int k=0;k<n;k++){
            ans.push_back(nums[k]*nums[k]);
        }
           for(int i=0;i<n-1;i++){
            for(int j=0;j<n-i-1;j++){
                if(ans[j]>ans[j+1]){
                    swap(ans[j],ans[j+1]);
                }
            }
        }
        return ans;
    }
};
