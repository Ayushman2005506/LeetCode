class Solution {
public:
    int maxFrequencyElements(vector<int>& nums) {
        unordered_map<int,int> freq;
        for(int i=0;i<nums.size();i++){
            freq[nums[i]]++;
        }
        vector<int> frequencies;
        for(auto& a:freq){
            frequencies.push_back(a.second);
        }
        int max=0;
        for(int i=0;i<frequencies.size();i++){
            if(frequencies[i]>max){
                max=frequencies[i];
            }
        }
        int count=0;
        for(int i=0;i<frequencies.size();i++){
            if(frequencies[i]==max){
                count+=frequencies[i];
            }
        }
        return count;
    }
};
