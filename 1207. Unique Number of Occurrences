class Solution {
public:
    bool uniqueOccurrences(vector<int>& arr) {
        unordered_map<int,int> freq;
        for(int i=0;i<arr.size();i++){
            freq[arr[i]]++;
        }
        vector<int> ans;
        for(auto& a:freq){
            ans.push_back(a.second);
        }
        sort(ans.begin(), ans.end());
        for(int i=0;i<ans.size()-1;i++){
            if(ans[i]==ans[i+1]){
                return false;
            }
        }
        return true;
    }
};
