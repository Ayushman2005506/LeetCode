class Solution {
public:
    int findChampion(vector<vector<int>>& grid) {
        for(int i=0;i<grid.size();i++){
            bool champion=true;
            for(int j=0;j<grid.size();j++){
                if( i!=j && grid[i][j]==0){
                    champion=false;
                    break;
                }
            }
            if(champion){
            return i;
           }
        }
        return -1;
    }
};
