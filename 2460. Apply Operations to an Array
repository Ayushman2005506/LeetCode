class Solution {
public:
    vector<int> applyOperations(vector<int>& nums) {
        vector<int>a;
        vector<int>b;
        vector<int>ans;
        for(int i=0;i<nums.size()-1;i++){
            if(nums[i]==nums[i+1]){
                nums[i]=nums[i]*2;
                nums[i+1]=0;
            }            
        }
        for(int i=0;i<nums.size();i++){
            if(nums[i]==0){
                b.push_back(nums[i]);
            }
            else{
                a.push_back(nums[i]);
            }
        }
        for(int i=0;i<a.size();i++){
            ans.push_back(a[i]);
        }
        for(int i=0;i<b.size();i++){
            ans.push_back(b[i]);
        }
        return ans;
    }
};
