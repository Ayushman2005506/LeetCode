class Solution {
public:
    int sumOfEncryptedInt(vector<int>& nums) {
        int ans=0;
        for(int i=0;i<nums.size();i++){
            int count=0;
            int max=0;

            while(nums[i]>0){
                int last_digit =nums[i]%10;
                if(last_digit>max){
                    max=last_digit;
                }
                count++;
                nums[i]/=10;
            }
            int sum=0;
            for(int j=0;j<count;j++){
                sum=sum*10+max;
            }
            ans+=sum;
        }
        return ans;
    }
};
