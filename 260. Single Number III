class Solution {
public:
    vector<int> singleNumber(vector<int>& nums) {
        unordered_map<int,int>freq;
        vector<int>ans;

        for(int i=0;i<nums.size();i++){
            freq[nums[i]]++;
        }
        for(auto x:freq){
            if(x.second==1){
                ans.push_back(x.first);
            }
        }
        return ans;
    }
};
