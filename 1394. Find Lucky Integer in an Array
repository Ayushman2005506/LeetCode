class Solution {
public:
    int findLucky(vector<int>& arr) {
        vector<int>lucky;
       int count=0;
       for(int i=0;i<arr.size();i++){
        for(int j=0;j<arr.size();j++){
            if(arr[i]==arr[j]){
                count++;
            }
        }
        if (count==arr[i]){
        lucky.push_back(arr[i]);
        }
        count=0;
       } 
       if(lucky.empty()) {
            return -1;
        }
       int maxLucky=lucky[0];
       for(int i=0;i<lucky.size();i++){
        if(lucky[i]>maxLucky){
            maxLucky=lucky[i];
        }
       }
       return maxLucky;
    }
};
