class Solution {
public:
    int maxProduct(vector<int>& nums) {
        int max_product=0;
        for(int i=0;i<nums.size();i++){
            for(int j=0;j<nums.size();j++){
                if(((nums[i]-1)*(nums[j]-1))>max_product && i!=j){
                    max_product=(nums[i]-1)*(nums[j]-1);
                }
            }
        }
        return max_product;
    }
};
