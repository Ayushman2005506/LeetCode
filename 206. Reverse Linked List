class Solution {
public:
    ListNode* reverseList(ListNode* head) {
        ListNode* temp=head;
        int count=0;
        while(temp!=nullptr){
            count++;
            temp=temp->next;
        }

        ListNode* prev=nullptr;
        ListNode* current=head;
        for(int i=0;i<count;i++){
            ListNode* nextNode=nextNode->next;
            current->next=prev;
            prev=current;
            current=nextNode;
        }
        return prev;
    }
};
