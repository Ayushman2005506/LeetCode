class Solution {
public:
    ListNode* middleNode(ListNode* head) {
        int count=0;
        ListNode* temp=head;

        while(temp!=nullptr){
            count++;
            temp=temp->next;
        }
        ListNode* remove=head;
        if(count%2!=0){
            int a=(count-1)/2;
            for(int i=0;i<a;i++){
                remove=remove->next;
            }
            return remove;
        }

        else{
            for(int i=0;i<(count/2);i++){
                remove=remove->next;
            }
            return remove;
        }
    }
};
