class Solution {
public:
    int duplicateNumbersXOR(vector<int>& nums) {
        unordered_map<int,int> freq;
        for(int i=0;i<nums.size();i++){
            freq[nums[i]]++;
        }
        int ans=0;
        for(auto& x:freq){
            if(x.second==2){
                ans=ans^x.first;
            }
        }
        return ans;
    }
};
