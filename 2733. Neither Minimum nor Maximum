class Solution {
public:
    int findNonMinOrMax(vector<int>& nums) {
        int n=nums.size();
        for(int i=0;i<n-1;i++){
            for(int j=0;j<n-i-1;j++){
                if(nums[j]>nums[j+1]){
                    swap(nums[j],nums[j+1]);
                }
            }
        }
        int ans=0;
        if(n>2){
            ans=nums[1];
            return ans;
        }
        return -1;
        
    }
};
