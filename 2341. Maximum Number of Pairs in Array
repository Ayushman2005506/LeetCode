class Solution {
public:
    vector<int> numberOfPairs(vector<int>& nums) {
        vector<int> result;
        int count=0;
        for(int i=0;i<nums.size();i++){
            if(nums[i]==-1)continue;
            for(int j=i+1;j<nums.size();j++){
                if(nums[j]==-1)continue;
                if(nums[i]==nums[j]){
                    count++;
                   nums[i]=-1;
                   nums[j]=-1;
                   break;
                }
            }
        }
        result.push_back(count);
        int n=0;
        for(int i=0;i<nums.size();i++){
            if(nums[i]!=-1){
                n++;
            }
        }
        result.push_back(n);
        return result;
    }
};
