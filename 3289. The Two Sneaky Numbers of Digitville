class Solution {
public:
    vector<int> getSneakyNumbers(vector<int>& nums) {
        vector<int> ans;
        for(int i=0;i<nums.size();i++){
            int num=nums[i];
            for(int j=i+1;j<nums.size();j++){
                if(nums[j]==num){
                    ans.push_back(nums[i]);
                    break;
                }
            }
            
        }
        return ans;
    }
};
