class Solution {
public:
    int countDistinctIntegers(vector<int>& nums) {
        vector<int> reverse;
        for(int i=0;i<nums.size();i++){
            reverse.push_back(nums[i]);
        }
        for(int i=0;i<nums.size();i++){
            int rev=0;
            while(nums[i]>0){
                rev= rev*10+(nums[i] % 10);
                nums[i]/=10;
            }
            reverse.push_back(rev);
        }

        unordered_set<int> s;
        for(int i=0;i<reverse.size();i++){
            s.insert(reverse[i]);
        }
        int ans=s.size();
        return ans;
    }
};
