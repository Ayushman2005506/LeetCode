class Solution {
public:
    int missingNumber(vector<int>& nums) {
        unordered_map<int,int>freq;
        int n=nums.size();
        int ans=0;
        for(int i=0;i<=n;i++){
            nums.push_back(i);
        }
        for(int i=0;i<nums.size();i++){
            freq[nums[i]]++;
        }
        for(auto x:freq){
            if(x.second==1){
                ans=x.first;
                break;
            }
        }
        return ans;
    }
};
