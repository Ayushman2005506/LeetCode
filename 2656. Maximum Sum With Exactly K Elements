class Solution {
public:
    int maximizeSum(vector<int>& nums, int k) {
        int count=0;
        for(int i=0;i<k;i++){
            int max=0;
            for(int j=0;j<nums.size();j++){
                if(nums[j]>nums[max]){
                    max=j;
                }
                
            }
            count+=nums[max];
            nums[max]=nums[max]+1;
        }
        return count;
    }
};
