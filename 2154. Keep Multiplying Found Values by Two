class Solution {
public:
    int findFinalValue(vector<int>& nums, int original) {
        int n=nums.size();
        for(int i=0;i<n-1;i++){
            for(int j=0;j<n-i-1;j++){
                if(nums[j]>nums[j+1]){
                    swap(nums[j],nums[j+1]);
                }
            }
        }
        for(int i=0;i<nums.size();i++){
            if(nums[i]==original){
                original=2*original;
            }
            else{
                continue;
            }
        }
        return original;
    }
};
